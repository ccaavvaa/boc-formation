{
	"$schema": "http://phenix.salviadeveloppement.com/json-schema/mdr-schema.json",
	"version": "1.0.0",
    "type": "object",
    "name": "PersonData",
    "theme": "person",
    "isTransient": true,
    "title": "Person Data View",
    "findClass": "Person",
    "findPageSize": 5,
    "findOptions": {
        "count": true
    },
    "properties":{
		"personId": {
			"type": "string",
			"format": "code"
		},
		"name": {
			"type": "string"
		},
		"firstName": {
			"type": "string"
		},
		"birthDate": {
			"type":"string",
			"format":"date"
		}
    }
}