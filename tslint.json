{
  "extends": "tslint:latest",
  "rules": {
    "no-bitwise": false,
    "eofline": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "no-console": [
      false
    ],
    "max-classes-per-file": [
      false, 1
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "ignore",
          "arrays": "ignore",
          "functions": "ignore",
          "typeLiterals": "ignore"
        },
        "singleline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ],
    "space-before-function-paren": false,
    "jsdoc-format": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-return-await": false
  }
}