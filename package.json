{
    "name": "boc-formation",
    "version": "0.0.1",
    "license": "MIT",
    "description": "bare server",
    "author": "Catalin VASCU",
    "main": "./index.js",
    "typings": "./index.d.ts",
    "nyc": {
        "reporter": [
            "lcov",
            "text-summary",
            "html"
        ],
        "include": [
            "lib/views/**/*.js",
            "lib/rules/**/*.js"
        ]
    },
    "scripts": {
        "test": "mocha ./test/**/*.test.js",
        "coverage": "nyc npm test",
        "start": "node index.js",
        "entity": "bo-code-gen ./schema/Models ./src/lib/models -m Basic -n Basic -c ./schema/code-gen/config.js",
        "view": "bo-code-gen ./schema/Views ./src/lib/views -n Memory -i ../models",
        "updep": "yarn upgrade logger boc-interfaces boc boc-server mdr-object-store server-commons sql-data-driver"
    },
    "dependencies": {
        "@phoenix/boc": "^3.0.0",
        "@phoenix/boc-interfaces": "^1.1.0",
        "@phoenix/boc-server": "^3.0.0",
        "@phoenix/codata": "^1.1.1",
        "@phoenix/logger": "^2.0.0",
        "@phoenix/mdr-object-store": "^1.1.0",
        "@phoenix/server-commons": "^1.0.0",
        "@phoenix/service-route": "^1.0.0",
        "@phoenix/sql-data-driver": "^1.1.0",
        "archiver": "^2.1.1",
        "body-parser": "^1.18.2",
        "cookie": "^0.3.1",
        "debug": "^3.1.0",
        "express": "^4.16.2",
        "file-url": "^2.0.2",
        "http-proxy": "^1.17.0",
        "lodash": "^4.17.4",
        "moment": "^2.22.2",
        "uuid": "^3.3.2",
        "winston": "^3.1.0",
        "winston-transport": "^4.2.0"
    },
    "devDependencies": {
        "@phoenix/bo-code-gen": "^2.3.0",
        "@phoenix/boc-prof": "^0.1.0",
        "@types/archiver": "^2.1.1",
        "@types/body-parser": "^1.16.7",
        "@types/chai": "^4.0.6",
        "@types/chai-http": "^3.0.3",
        "@types/cookie": "^0.3.1",
        "@types/cookie-parser": "^1.4.1",
        "@types/debug": "0.0.29",
        "@types/express": "^4.0.39",
        "@types/file-url": "^2.0.0",
        "@types/lodash": "^4.14.86",
        "@types/mocha": "^2.2.44",
        "@types/moment": "^2.13.0",
        "@types/node": "^8.0.53",
        "@types/uuid": "^3.4.4",
        "chai": "^4.1.2",
        "chai-http": "^3.0.0",
        "del": "^3.0.0",
        "dotbin": "^1.0.1",
        "express-request-mock": "^1.1.0",
        "fancy-log": "^1.3.2",
        "gulp": "^4.0.0",
        "gulp-copy": "^1.0.1",
        "gulp-exec": "^2.1.3",
        "gulp-install": "^1.1.0",
        "gulp-mocha": "^4.3.1",
        "gulp-rename": "^1.2.2",
        "gulp-sourcemaps": "^2.6.1",
        "gulp-tslint": "^8.1.2",
        "gulp-typescript": "^3.2.3",
        "merge2": "^1.2.0",
        "mocha": "^4.0.1",
        "node-mocks-http": "^1.6.6",
        "nyc": "^13.1.0",
        "source-map-support": "^0.5.9",
        "tslint": "^5.8.0",
        "typescript": "^3.0.0"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "repository": {
        "type": "git",
